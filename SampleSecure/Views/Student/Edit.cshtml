@model SampleSecure.Models.Student

@{
    ViewData["Title"] = "Edit Barang";
}

<h2>Edit Mahasiswa</h2>

<form asp-action="Edit" method="post">
    <div class="form-group">
        <label asp-for="Kode"></label>
        <input asp-for="Kode" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="NamaBarang"></label>
        <input asp-for="NamaBarang" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Simpan</button>
    <a asp-action="Index" class="btn btn-secondary">Batal</a>
</form>
